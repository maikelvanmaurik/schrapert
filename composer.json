{
    "name": "maikelvanmaurik/schrapert",
    "authors": [
        {
            "name": "Maikel van Maurik",
            "email": "maikelvanmaurik@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Schrapert\\": "src/Schrapert/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Schrapert\\Tests\\": "tests/"
        }
    },
    "bin": [
        "schrapert"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0",
        "react/event-loop": "^0.4",
        "react/promise": "~2.0",
        "react/stream": "~0.4",
        "react/http-client": "^0.5",
        "react/dns": "^0.4.2",
        "react/filesystem": "dev-master",
        "psr/http-message": "~1.0",
        "psr/log": "^1.0.2",
        "psr/container": "^1.0@dev",
        "illuminate/container": "^8.0@dev",
        "psr/event-dispatcher": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "clue/block-react": "^1.1",
        "squizlabs/php_codesniffer": "^3.0@dev"
    }
}
